{
  "name": "questioner-andela",
  "version": "1.0.0",
  "description": "Andela Developer Challenge Cycle 40 Bootcamp",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && babel -d ./build ./server -s",
    "clean": "rm -rf build && mkdir build",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text --cache npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "heroku-postbuild": "npm run build",
    "lint": "node_modules/.bin/eslint ./server/*.js",
    "lint-fix": "node_modules/.bin/eslint --fix ./server --ext .js --color",
    "migrations": "babel-node ./server/migrations/drop.js && babel-node ./server/migrations/init.js --",
    "prod": "node build/app.js",
    "start": "node_modules/.bin/nodemon server/app.js --exec babel-node -- ",
    "test": "npm run migrations && nyc mocha server/tests/**/*spec.js --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chikeozulumba/questioner-andela.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chikeozulumba/questioner-andela/issues"
  },
  "homepage": "https://github.com/chikeozulumba/questioner-andela#readme",
  "dependencies": {
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "pg": "^7.7.1",
    "supertest": "^3.3.0",
    "url-regex": "^4.1.1",
    "validatorjs": "^3.15.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "sinon": "^7.2.2",
    "sinon-express-mock": "^2.1.0"
  }
}
